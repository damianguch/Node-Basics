<%- include('partials/header.ejs') %>

<h3>Welcome to the Home Page</h3>
<h4>List of Products:</h4>
<ul>
  <% if(products && products.length > 0) { products.forEach(product=> {%>
  <li><%= product.name %> - $<%= product.price %></li>
  <% }) } else { %>
  <li>No Products Available</li>
  <% } %>
</ul>
